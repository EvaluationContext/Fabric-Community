	measure 'VS alt Text' = ```
			
			VAR _CurrentCity = "Briarport"
			VAR _SelectedCity = [Selected City]
			
			// Determine the winner for each category (Returns TRUE if Briarport Wins)
			// Housing: Lower is Better
			VAR _Win_Housing = [Housing Affordability vs Selected City] > 0
			// Emissions: Lower is Better
			VAR _Win_Emissions = [Emissions vs Selected City] > 0
			// Green Space: Higher is Better
			VAR _Win_GreenSpace = [Green Space vs Selected City] < 0
			// Transit: Higher is Better
			VAR _Win_Transit = [Transit vs Selected City] < 0
			// Livability: Higher is Better
			VAR _Win_Livability = [Livability vs Selected City] < 0
			
			// Count Briarport Wins (Convert Boolean to Integer ABS)
			VAR _Score = 
			    ABS(INT(_Win_Housing)) + 
			    ABS(INT(_Win_Emissions)) + 
			    ABS(INT(_Win_GreenSpace)) + 
			    ABS(INT(_Win_Transit)) + 
			    ABS(INT(_Win_Livability))
			
			// Determine Overall Winner
			VAR _Winner = IF(_Score >= 3, _CurrentCity, _SelectedCity)
			
			// Build list of strengths for the winner
			VAR _WinnerStrengths = 
			    CONCATENATEX(
			        FILTER(
			            {
			                (1, "Housing Affordability", _Win_Housing),
			                (2, "Emissions", _Win_Emissions),
			                (3, "Green Space", _Win_GreenSpace),
			                (4, "Transit Score", _Win_Transit),
			                (5, "Livability", _Win_Livability)
			            },
			            // If Briarport (Current) is winner, filter for True. If Selected is winner, filter for False
			            [Value3] = IF(_Winner = _CurrentCity, TRUE(), FALSE())
			        ),
			        [Value2],
			        ", ",
			        [Value1] // Order by predetermined index
			    )
			
			RETURN
			    "Comparison chart between " & _CurrentCity & " and " & _SelectedCity & ". " & 
			    "The overall winner is " & _Winner & ". " & 
			    _Winner & " performs better in the following categories: " & _WinnerStrengths & "."
			
			```
